int findMaxConsecutiveOnes(int* nums, int numsSize){

    for(int i=1; i<numsSize; i++)
    {
        if(nums[i-1]>=1 && nums[i]==1)
        {
            nums[i]=nums[i]+nums[i-1];
        }
    }
    
    int max=0;
    
    for(int i=0; i<numsSize; i++)
    {
        if (nums[i]>max){
            max = nums[i];
        }
    }
    return max;
}
